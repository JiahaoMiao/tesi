      \field{extraname}{2}
      \field{sortinit}{C}
      \field{sortinithash}{4d103a86280481745c9c897c925753c0}
      \field{labelnamesource}{author}
      \field{labeltitlesource}{title}
      \field{abstract}{We calculate the thrust distribution in e+e− annihilation including resummation of leading and next-to-leading logarithms of 1−T to all orders in QCD perturbation theory. This enables us to predict the distribution up to the largest values of T and reduces the renormalization scale dependence relative to the leading order at both large and small T. From a comparison with experimental data, we find Λ(5)MS=200±140 MeV, corresponding to αs(m2z)=0.116±0.012.}
      \field{issn}{0370-2693}
      \field{journaltitle}{Physics Letters B}
      \field{number}{3}
      \field{title}{Thrust distribution in e+e− annihilation}
      \field{volume}{263}
      \field{year}{1991}
      \field{pages}{491\bibrangedash 497}
      \range{pages}{7}
      \verb{doi}
      \verb https://doi.org/10.1016/0370-2693(91)90494-B
      \endverb
      \verb{urlraw}
      \verb https://www.sciencedirect.com/science/article/pii/037026939190494B
      \endverb
      \verb{url}
      \verb https://www.sciencedirect.com/science/article/pii/037026939190494B
      \endverb
